{
  "kind": "collectionType",
  "collectionName": "chain-wallets",
  "info": {
    "singularName": "chain-wallet",
    "pluralName": "chain-wallets",
    "displayName": "Chain Wallet",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "privateAttributes": [
      "seed",
      "managed",
      "key",
      "encKey",
      "publishedAt",
      "createdAt",
      "updatedAt",
      "publishedBy",
      "createdBy",
      "updatedBy"
    ]
  },
  "pluginOptions": {},
  "attributes": {
    "address": {
      "type": "string",
      "required": true
    },
    "chain": {
      "type": "enumeration",
      "enum": [
        "homestead",
        "goerli",
        "sepolia",
        "arbitrum",
        "arbitrum-goerli",
        "avalanche-mainnet",
        "avalanche-fuji",
        "matic",
        "maticmum",
        "optimism",
        "optimism-goerli"
      ],
      "default": "homestead",
      "required": true
    },
    "name": {
      "type": "string",
      "required": false
    },
    "seed": {
      "type": "text",
      "private": true,
      "unique": false
    },
    "managed": {
      "type": "boolean",
      "private": true,
      "default": true
    },
    "primary": {
      "type": "boolean",
      "private": true,
      "default": true
    },
    "key": {
      "type": "string",
      "private": true,
      "unique": false
    },
    "encKey": {
      "type": "password"
    },
    "tokens": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::chain-wallets.chain-token",
      "mappedBy": "wallet"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "wallets"
    }
  }
}
